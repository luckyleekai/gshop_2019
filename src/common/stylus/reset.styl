// css reset
body, p, ul, h1, h2, h3, h4, h5, h6
  margin: 0
ul
  padding: 0
li
  list-style: none

@font-face
  font-family: "iconfont"
  src url('../fonts/iconfont.eot?t=1556155274867'); /* IE9 */
  src url('../fonts/iconfont.eot?t=1556155274867#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('../fonts/iconfont.woff?t=1556155274867') format('woff'),
  url('../fonts/iconfont.ttf?t=1556155274867') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('../fonts/iconfont.svg?t=1556155274867#iconfont') format('svg'); /* iOS 4.1- */

.iconfont 
  font-family: "iconfont" !important
  font-size: 16px
  font-style: normal
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale
